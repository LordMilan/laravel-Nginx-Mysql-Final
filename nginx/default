
server { 

    listen       80;
    server_name  localhost;

    location ^~ / {
    proxy_pass http://laravelfinal:8000;
  }
  location ^~ /css {
    proxy_pass http://laravelfinal:8000;
  }
 
    
    location ~ \.php$ {
        include snippets/fastcgi-php.conf;
        fastcgi_pass unix:/var/run/php/php7.4-fpm.sock;
     }

  }